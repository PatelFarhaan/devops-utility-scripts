# AWS Credentials
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1

# Auto Scaling (for eb_update_config.py)
ASG_NAME=your-auto-scaling-group-name

# PostgreSQL (for pg2mongo migration)
PG_DATABASE=webapp
PG_USER=your_pg_user_here
PG_PASSWORD=your_pg_password_here
PG_HOST=localhost
PG_PORT=5432

# MongoDB (for pg2mongo migration)
MONGO_URI=mongodb://localhost:27017/
MONGO_DATABASE=admin

# MongoDB (for mongo_service.sh)
MONGO_USERNAME=admin
MONGO_PASSWORD=your_mongo_password_here

# Jenkins Prometheus Metrics
JENKINS_METRICS_USERNAME=your_jenkins_username_here
JENKINS_METRICS_PASSWORD=your_jenkins_password_here

# Grafana
GRAFANA_USERNAME=admin
GRAFANA_PASSWORD=your_grafana_password_here
